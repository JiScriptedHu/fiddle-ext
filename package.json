      },
      {
        "command": "fiddle-ext.runFiddle",
        "title": "Fiddle: Run Fiddle"
      },
      {
        "command": "fiddle-ext.packFiddle",
        "title": "Fiddle: Package Fiddle"
      },
      {
        "command": "fiddle-ext.installerFiddle",
        "title": "Fiddle: Make Installer for Fiddle"
      },
      {
        "command": "fiddle-ext.electronAPI",
        "title": "Fiddle: Electron API"
      },
      {
        "command": "fiddle-ext.toggleDeveloper",
        "title": "Fiddle: Developer Tools"
      },
      {
        "command": "fiddle-ext.repository",
        "title": "Fiddle: Repository"
      },
      {
        "command": "fiddle-ext.about",
        "title": "Fiddle: About Fiddle Extension"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@electron/fiddle-core": "^1.3.4"
  }
}
