{
  "name": "fiddle-ext",
  "displayName": "Fiddle",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fiddle-ext.helloWorld",
        "title": "Fiddle: Hello World"
      },
      {
        "command": "fiddle-ext.installElectron",
        "title": "Fiddle: Install Electron Version"
      },
      {
        "command": "fiddle-ext.removeElectron",
        "title": "Fiddle: Remove Electron Version"
      },
      {
        "command": "fiddle-ext.newFiddle",
        "title": "Fiddle: New Fiddle"
      },
      {
        "command": "fiddle-ext.newTest",
        "title": "Fiddle: New Test"
      },
      {
        "command": "fiddle-ext.newWindow",
        "title": "Fiddle: New Window"
      },
      {
        "command": "fiddle-ext.open",
        "title": "Fiddle: Open"
      },
      {
        "command": "fiddle-ext.openRecent",
        "title": "Fiddle: Open Recent"
      },
      {
        "command": "fiddle-ext.save",
        "title": "Fiddle: Save"
      },
      {
        "command": "fiddle-ext.saveAs",
        "title": "Fiddle: Save as"
      },
      {
        "command": "fiddle-ext.exportGist",
        "title": "Fiddle: Publish to Gist"
      },
      {
        "command": "fiddle-ext.importGist",
        "title": "Fiddle: Load Gist"
      },
      {
        "command": "fiddle-ext.saveAsForgeProject",
